<h1> Adoption Application </h1>

<%= form_for @adoption_application do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :address %>:
  <%= f.text_field :address %><br />

  <%= f.label :city %>:
  <%= f.text_field :city %><br />

  <%= f.label :cell %>:
  <%= f.text_field :cell %><br />

  <%= f.label :email %>:
  <%= f.text_field :email %><br />

  <%= f.submit %>
<% end %>
